# OpenAI API key used to call GPT for itinerary generation
OPENAI_API_KEY=your-openai-api-key

# Supabase credentials (optional; used if you persist plans)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key

# Gmail credentials for nodemailer
GMAIL_USER=your.email@gmail.com
GMAIL_PASS=your-gmail-app-password
GMAIL_CLIENT_ID=your-google-oauth-client-id
GMAIL_CLIENT_SECRET=your-google-oauth-client-secret
GMAIL_REFRESH_TOKEN=your-google-oauth-refresh-token

# Stripe keys (optional; enable premium upsells)
STRIPE_PUBLIC_KEY=
STRIPE_SECRET_KEY=